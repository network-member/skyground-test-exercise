{
  "name": "express-sample",
  "version": "1.0.0",
  "main": "src/server.ts",
  "author": "Vlad Kazantsev",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register ./src/server.ts",
    "migrations:sync": "prisma migrate dev --schema src/prisma --name",
    "prisma:generate-client": "prisma generate --schema src/prisma",
    "start": "yarn build && TS_NODE_BASEURL=./build node -r tsconfig-paths/register ./build/src/server.js",
    "build": "tsc --pretty",
    "check-types": "tsc --noEmit",
    "postinstall": "husky"
  },
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/cors": "^2",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1",
    "@types/node": "^20.14.6",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2",
    "prisma": "^5.15.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@4.3.0",
  "engines": {
    "node": "20.*",
    "npm": "please-use-yarn",
    "yarn": "4.*"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
